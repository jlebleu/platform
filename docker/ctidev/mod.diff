+	} else {
+		team = result.Data.(*model.Team)
+	}
+
+	phoneState := PhoneStateFromJson(r.Body)
+
+	l4g.Debug("updatePhonestatus phone state %v", phoneState.Username)
+
+	username := phoneState.Username
+	status := strconv.Itoa(phoneState.Status)
+
